<html>
    <% if flash[:error]%>
    <p><%= flash[:error]%></p>
    <%end%>
    <form action="/login" method="post">
        <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
        <input type="text" name="Email">
        <input type="text" name="Password">
        <input type="submit" value="Login">
    </form>
</html>
